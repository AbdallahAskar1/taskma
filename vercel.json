{
    "builds": [
      {
        "src": "backend/src",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/app",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "dest": "/backend/dist/$1"
      },
      {
        "src": "/",
        "methods": ["GET"],
        "dest": "/frontend/dist"
      }
    ]
  }
  